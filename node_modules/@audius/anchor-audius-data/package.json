{
  "name": "@audius/anchor-audius-data",
  "version": "0.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "anchor build",
    "build": "rollup -c",
    "build-dev": "./scripts/build-dev.sh",
    "deploy-dev": "anchor deploy",
    "dev": "rollup -c --watch",
    "dev-setup": "npm run build-dev && npm run deploy-dev && npm run seed-tx",
    "install-dev": "./scripts/install-dev-dependencies.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "localnet-down": "killall -9 solana-test-validator",
    "localnet-up": "solana-test-validator --reset --limit-ledger-size 100000",
    "localnet-up-ci": "solana-test-validator --reset --limit-ledger-size 100000 --quiet &",
    "parse-tx": "./scripts/parse-tx.sh",
    "seed-tx": "./scripts/seed-tx.sh",
    "test": "BROWSER= anchor test"
  },
  "dependencies": {
    "@project-serum/anchor": "0.24.1"
  },
  "devDependencies": {
    "@project-serum/anchor-cli": "0.24.1",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.1",
    "@tsconfig/node16-strictest": "^1.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/commander": "^2.12.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "@types/secp256k1": "^4.0.3",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commander": "^8.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "keccak256": "1.0.2",
    "mocha": "^9.0.3",
    "prettier": "^2.5.1",
    "prettier-config-standard": "^5.0.0",
    "rollup": "2.70.1",
    "rollup-plugin-dts": "4.2.0",
    "secp256k1": "4.0.2",
    "standard": "^16.0.4",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.6.2",
    "web3": "1.2.8"
  },
  "description": "Audius solana anchor js bindings",
  "author": "Audius",
  "license": "Apache-2.0"
}
